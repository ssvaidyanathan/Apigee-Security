<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OAuthToken_Response">
    <DisplayName>OAuthToken_Response</DisplayName>
    <Attributes>
        <Attribute name="subject.preferred_username" ref="subject.preferred_username" display="false"/>
        <Attribute name="subject.email" ref="subject.email" display="false"/>
        <Attribute name="subject.sub" ref="subject.sub" display="false"/>
        <Attribute name="subject.name" ref="subject.name" display="false"/>
        <Attribute name="subject.locale" ref="subject.locale" display="false"/>
        <Attribute name="subject.family_name" ref="subject.family_name" display="false"/>
        <Attribute name="subject.given_name" ref="subject.given_name" display="false"/>
        <Attribute name="subject.access_token" ref="access_token" display="false"/>
    </Attributes>
    <StoreToken>true</StoreToken>
    <Operation>GenerateAccessToken</Operation>
    <ExternalAuthorization>true</ExternalAuthorization>
    <!-- ExternalAccessToken>access_token</ExternalAccessToken -->
    <ExpiresIn>1800000</ExpiresIn>
    <!-- 30 minutes -->
    <SupportedGrantTypes>
        <GrantType>client_credentials</GrantType>
    </SupportedGrantTypes>
    <GenerateResponse enabled="true"/>
</OAuthV2>