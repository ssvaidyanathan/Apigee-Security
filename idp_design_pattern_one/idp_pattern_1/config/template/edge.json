{
    "version": "1.0",
    "envConfig": {
        "test": {
            "caches": [
                {
                    "name": "ATZ_CODE_STATE_CACHE",
                    "expirySettings":{
                        "timeoutInSec": {
                          "value": "idp.atz_code.cache.ttl.value"
                        }
                      },
                    "description": "Authorization Code and State Cache"
                },
                {
                    "name": "JWKS_CACHE",
                    "expirySettings":{
                        "timeoutInSec": {
                          "value": "idp.jwks.cache.ttl.value"
                        }
                      },
                    "description": "JWKS Response Cache"
                }
            ],
            "kvms": [
                {
                    "name": "IdP_OIDC_Info",
                    "entry": [
                        {"name": "idp.client.id","value": "idp.client.id.value"},
                        {"name": "idp.scope","value": "idp.scope.value"},
                        {"name": "idp.subject","value": "idp.subject.value"},
                        {"name": "idp.issuer","value": "idp.issuer.value"},
                        {"name": "idp.userinfo.endpoint","value": "idp.userinfo.endpoint.value"},
                        {"name": "idp.atz_code_to_token.endpoint","value": "idp.atz_code_to_token.endpoint.value"},
                        {"name": "idp.hostname","value": "idp.hostname.value"},
                        {"name": "idp.jwks.endpoint","value": "idp.jwks.endpoint.value"},
                        {"name": "idp.client.secret","value": "idp.client.secret.value"},
                        {"name": "idp.atz_code.endpoint","value": "idp.atz_code.endpoint.value"},
                        {"name": "idp.audience","value": "idp.audience.value"},
                        {"name": "idp.redirect_uri","value": "idp.redirect_uri.value"}
                    ]
                }
            ]
        }
    }
}
